{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/rich-media-upload",
	"version": "0.1.0",
	"title": "Rich Media Upload",
	"category": "widgets",
	"icon": "admin-media",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"reusable": false,
  
		"html": false,
		"color": {
		  "gradients": true,
		  "link": true,
		  "__experimentalDefaultControls": {
			"background": true,
			"text": true,
			"link": true
		  }
		},
		"spacing": {
		  "margin": true
		},
		"typography": {
		  "fontSize": true,
		  "lineHeight": true,
		  "__experimentalFontFamily": true,
		  "__experimentalFontWeight": true,
		  "__experimentalFontStyle": true,
		  "__experimentalTextTransform": true,
		  "__experimentalLetterSpacing": true,
		  "__experimentalDefaultControls": {
			"fontSize": true,
			"fontAppearance": true,
			"textTransform": true
		  }
		}
	  },
	"textdomain": "rich-media-upload",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
        "imageUrl": {
            "type": "string",
            "source": "attribute",
            "attribute": "src",
            "selector": "img"
        },
        "imageId": {
            "type": "number"
        },
        "imageAlt": {
            "type": "string",
            "source": "attribute",
            "attribute": "alt",
            "selector": "img"
        }

    }
}
